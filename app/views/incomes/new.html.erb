<h1>収入を追加する</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
  <%= form_for(@income) do |f| %>
    <%= f.label :なんの収入? %>
    <%= f.text_field :name %>

    <%= f.label :金額 %>
    <%= f.number_field :receipt %>

    <%= f.submit "決定", class: "btn btn-primary" %>
  <% end %>
  </div>
</div>